<table id="serverlist" cellpadding="0" cellspacing="0">
  <thead>
    <TR>
      <th>UUID</th>
      <tH>Serial Number</th>
      <th>Manufacturer</th>
      <th>Productname</th>
      <tH>Asset Tags</th>
      <th>Location</tH>
      <TH>Action</tH>
    </tr>
  </thead>
  <tbody>
    <% @dcb_conn.list.each do |dc| %>
      <tr>
      <td><%= dc["uuid"] %></td>
      <td><%= dc["serial_no"] %></td>
      <td><%= dc["manufacturer"] %></td>
      <td><%= dc["product_name"]%></td>
      <td><%= dc["asset_tags"]%></td>
      <td><%= dc["location"]%></td>
      <td><span class="button">&#x270E;</span>&nbsp;<span class="button">&#x2717;</span></td>
    </tr>
  <% end %>
</tbody>
</table>
