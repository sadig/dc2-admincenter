<table cellpadding="0" cellspacing="0" class="table table-striped">
  <thead>
    <tr>
      <th><input id="check_all" type="checkbox" name="check_all"/></th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Height</th>
      <th>Fact Sheet</th>
    </tr>
  </thead>
  <tbody>
    <% @cabinetlist.each do |cabinet| %>
      <tr url="<%= edit_admin_assetmgmt_cabinet_path(cabinet) %>">
        <td><input class="del_check" type="checkbox" name="check" value="<%= admin_assetmgmt_cabinet_path(cabinet) %>"/></td>
        <td class="data-cell"><%= cabinet.manufacturer %></td>
        <td class="data-cell"><%= cabinet.model %></td>
        <td class="data-cell"><%= cabinet.height %></td>
        <td><a target="_blank" href="<%= cabinet.url %>">Fact Sheet</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div id="cabinetbtn" class="btn-group pull-right">
    <input type="button" name="btn_add" value="Add" class="btn"/>
    <input type="button" name="btn_refresh" value="Refresh" class="btn"/>
    <input type="button" name="btn_delete" value="Delete" class="btn"/>
</div>
